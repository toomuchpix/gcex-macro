<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="395.0" prefWidth="335.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.gcex.Controller">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <TextField fx:id="fname" onAction="#onChangeFileName" onInputMethodTextChanged="#onChangeFileName" prefHeight="25.0" prefWidth="248.0" promptText="fname" />
    <TextField fx:id="param" onAction="#onChangeParams" promptText="params" />
    <Button fx:id="btn1" onAction="#onStartButtonClick" text="starten" />
    <Label fx:id="sText" text="-" />
    <Label fx:id="m1" text="&lt;M1&gt;" />
    <Label fx:id="ma" text=" " />
    <TableView fx:id="mt" prefHeight="200.0" prefWidth="200.0">
     <columns>
         <TableColumn fx:id="ti" maxWidth="30.0" prefWidth="30.0" text="ID" />
         <TableColumn fx:id="tn" maxWidth="200.0" prefWidth="100.0" text="Name" />
         <TableColumn fx:id="tl" maxWidth="50.0" prefWidth="50.0" text="Zeilen" />
     </columns>
   </TableView>
</VBox>
